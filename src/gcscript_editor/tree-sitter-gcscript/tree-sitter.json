{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gcscript",
      "camelcase": "GCScript",
      "title": "gcscript",
      "scope": "source.gcscript",
      "file-types": [
        "gc"
      ],
      "injection-regex": "^gcscript$",
      "class-name": "TreeSitterGcscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A languguge that compiles to Desmos",
    "authors": [
      {
        "name": "john",
        "email": "nil"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-gcscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}